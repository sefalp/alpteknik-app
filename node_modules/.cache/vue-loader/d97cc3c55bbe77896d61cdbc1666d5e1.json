ss=\"grid-container\">\n\n        <header class=\"header\"> \n\n            <div class=\"burger\">\n                <div class=\"line1\"></div>\n                <div class=\"line2\"></div>\n                <div class=\"line3\"></div>\n            </div>\n\n            <div class=\"brand\">\n                <a href=\"/\"> Alp Teknik Hırdavat </a>\n            </div>\n\n                <div class=\"costumer-service\">\n                    <ul>\n                        <a href=\"/\">Sepetim</a>\n                        <a href=\"/\">Giriş Yap</a>\n                    </ul>\n                </div>\n\n\n        </header>\n\n        <main class=\"main\">\n\n                <ul class=\"navLinks\">\n                    <li>\n                        <a href='#'>Araba Anahtarı</a>\n                    </li>\n                    <li>\n                        <a href='#'>El Aletleri</a>\n                    </li>\n                    <li>\n                        <a href='#'>Sprey Boyalar</a>\n                    </li>\n                    <li>\n                        <a href='#'>Matkaplar</a>\n                    </li>\n                    <li>\n                        <a href='#'>Musluklar</a>\n                    </li>\n\n                </ul>\n\n        </main>\n\n        <footer class=\"footer\">\n                <h2>Hakları Saklıdır</h2>\n        </footer>\n\n    </div>\n\n</template>\n\n<script>\n\n\nexport default {\n  name: 'HelloWorld',\n  props: {\n    msg: String\n  }\n}\n\nconst navSlide = () =>{\n\n    const burger = document.querySelector('.burger')\n    const navi = document.querySelector('.navLinks')\n    const navLinks = document.querySelectorAll('.navLinks li')\n    \n    burger.addEventListener('click',()=>{\n\n        navi.classList.toggle('nav-active');\n\n        navLinks.forEach((link, index) => {\n        \n            if(link.style.animation){\n                link.style.animation = ''\n    \n            }\n            else{\n                link.style.animation = `navLinkFade 0.5s ease forwards ${ index / 7 + 0.5}s`;\n            }\n        })\n        burger.classList.toggle('toggle');\n    })\n\n\n}\n\nnavSlide();\n\n\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n*{\n    margin: 0px;\n    padding: 0px;\n    box-sizing: border-box;\n}\n\nhtml{\n    font-size: 62.5%;\n    height: 100%;\n}\n\nbody{\n    font: 1.6rem;\n    font-family: 'Poppins', sans-serif;\n    height: 100%;\n    margin: 0;\n}\n\n.grid-container{\n    display: grid;\n    grid-template-areas: \n    \"header\"\n    \"main\"\n    \"footer\";\n    grid-template-columns: 1fr;\n    grid-template-rows: 5rem 1fr 4rem;\n    height: 100%;\n}\n\n/* header part */\n\n.header{\n    grid-area: header;\n    width: 100%;\n    background-color:  rgb(30, 85, 78);\n    display: flex;\n    justify-content: space-around;\n    align-items: center;\n    height: 5rem;\n    padding-left: 2rem;\n\n}\n\n.burger{\n    border: .1rem rgb(255, 255, 255) solid;\n    cursor: pointer;\n}\n\n.burger div{\n    width: 25px;\n    height: 2px;\n    background-color: rgb(226, 226, 226);\n    margin: 5px;\n    transition: all 0.3s ease;\n}\n\n\n.brand{\n    width: 50%;\n}\n\n.brand a{\n    color: white;\n    text-decoration: none;\n    font-size: 3rem;\n    padding-left: 3rem;\n}\n\n.brand a:hover{\n    color: #ff8000\n}\n\n.costumer-service{\n    display: flex;\n    justify-content: space-around;\n    width: 20%;\n}\n\n.costumer-service a{\n    color: white;\n    text-decoration: none;\n    font-size: 2rem;\n    padding-right: 3rem;\n}\n.costumer-service a:hover{\n    color: #ff8000\n}\n\n\n/*main part */\n\n\n.main{\n    grid-area: main;\n    background-color: rgb(255, 255, 255);\n}\n\n.navLinks {\n    height: 100%;\n    width: 16%;\n    background-color: rgb(30, 85, 78);\n    display: flex;\n    flex-direction: column;\n    transform: translateX(-100%);\n    transition: transform 0.5s ease-in;\n    justify-content: space-around;\n}\n\n.nav-active{\n    transform: translateX(0%);\n}\n\n.navLinks li{\n    padding: 4rem; \n    opacity: 0;\n}\n\n.navLinks a{\n    font-size: 2.5rem;\n    text-decoration: none;\n    color: white;\n}\n.navLinks a:hover{\n    font-size: 2.5rem;\n    text-decoration: none;\n    color: #ff8000;\n}\n\n@keyframes navLinkFade {\n    from {opacity: 0;\n          transform: translateX(-50px);      \n        }\n    to {opacity: 1;\n        transform: translateX(0px);      \n      }\n  }\n\n  .toggle .line1{\n      transform: rotate(-45deg) translate(-5px,5px);\n      \n  }\n  .toggle .line2{\n    opacity: 0;\n    }\n    .toggle .line3{\n        transform: rotate(45deg) translate(-5px,-5px);\n    \n    }\n/* footer part */\n.footer{\n    grid-area: footer;\n    font-size: 1.5rem;\n    color: white;\n    background-color: rgb(30, 85, 78);\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    padding-top: 1rem;\n}\n</style>\n"]}]}